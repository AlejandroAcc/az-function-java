{
  "scriptFile": "../functions-coffee-order.jar",
  "entryPoint": "com.microsoft.serverless.coffeeorder.CoffeeMaker.onResponse",
  "bindings": [
    {
      "type": "eventHubTrigger",
      "name": "response",
      "direction": "in",
      "path": "funccoffeemaker",
      "connection": "AzureWebJobsSampleEventHub"
    },
    {
      "type": "table",
      "name": "order",
      "direction": "in",
      "tableName": "orders",
      "partitionKey": "CustomerOrder",
      "rowKey": "{orderId}",
      "connection": "AzureWebJobsSampleStorage"
    },
    {
      "type": "table",
      "name": "inventory",
      "direction": "in",
      "tableName": "inventory",
      "partitionKey": "Remaining",
      "rowKey": "{coffee}",
      "connection": "AzureWebJobsSampleStorage"
    }
  ]
}
