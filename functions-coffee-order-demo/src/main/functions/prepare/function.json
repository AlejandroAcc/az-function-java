{
  "scriptFile": "../functions-coffee-order.jar",
  "entryPoint": "com.microsoft.serverless.coffeeorder.CoffeeMaker.prepare",
  "bindings": [
    {
      "type": "queueTrigger",
      "name": "pending",
      "direction": "in",
      "queueName": "pendingorders",
      "connection": "AzureWebJobsSampleStorage"
    },
    {
      "type": "table",
      "name": "order",
      "direction": "in",
      "tableName": "orders",
      "partitionKey": "CustomerOrder",
      "rowKey": "{queueTrigger}",
      "connection": "AzureWebJobsSampleStorage"
    }
  ]
}
